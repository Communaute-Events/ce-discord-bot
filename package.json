{
  "name": "ce-discord-bot",
  "version": "1.1.0",
  "description": "Communaut√© Event's discord bot",
  "main": "dist/src/index.js",
  "scripts": {
    "start": "node dist/src/index.js",
    "copy-files": "copyfiles -u 1 \"src/**/*.png\" \"src/**/*.yml\" \"src/**/*.yaml\" \"src/**/*.json\" ./*.env dist/",
    "build": "rimraf dist/ && tsc && tsc-alias && merge-dirs ./ dist/ --skip",
    "build:watch": "tsc && (concurrently \"tsc -w\" \"tsc-alias -w\" \"npm run copy-files\")",
    "url": "ts-node -r tsconfig-paths/register src/core/url.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "copyfiles": "^2.4.1",
    "merge-dirs": "^0.2.1",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "cross-fetch": "^4.0.0",
    "discord.js": "^14.13.0",
    "dotenv": "^16.3.1",
    "js-yaml": "^4.1.0",
    "mongodb": "^6.2.0",
    "ora": "^3.2.0",
    "ws": "^8.14.2"
  }
}
